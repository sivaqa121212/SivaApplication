package com.POM;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class WelcomeMercuryTours 
{
	// Object Repository
	
	@FindBy(linkText="SIGN-ON")
	WebElement SignOn;
	
	public void SignOn()
	{
		SignOn.click();
	}
	
	@FindBy(linkText="REGISTER")
	WebElement Register;
	
	public void register()
	{
	Register.click();
	}
	
	
}
